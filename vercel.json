{
  "version": 2,
  "builds": [
    {
      "src": "api/index.ts",
      "use": "@vercel/node"
    },
    {
      "src": "package.json",
      "use": "@vercel/static-build",
      "config": {
        "distDir": "dist/public"
      }
    }
  ],
  "routes": [
    {
      "src": "/api/admin/list",
      "dest": "api/admin/list.ts"
    },
    {
      "src": "/api/admin/create",
      "dest": "api/admin/create.ts"
    },
    {
      "src": "/api/admin/login",
      "dest": "api/admin/login.ts"
    },
    {
      "src": "/api/admin/logout",
      "dest": "api/admin/logout.ts"
    },
    {
      "src": "/api/admin/stats",
      "dest": "api/admin/stats.ts"
    },
    {
      "src": "/api/admin/(.*)",
      "dest": "api/admin/delete.ts"
    },
    {
      "src": "/api/(.*)",
      "dest": "api/index.ts"
    },
    {
      "src": "/assets/(.*)",
      "dest": "/assets/$1"
    },
    {
      "src": "/(.*)",
      "dest": "/index.html"
    }
  ]
}
